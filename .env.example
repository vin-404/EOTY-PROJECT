# Example environment configuration for Stock Trading Simulator
# Copy this file to `.env` and replace the placeholder values with your own

# Finnhub API key (get one for free at https://finnhub.io/)
FINNHUB_API_KEY=your_api_key_here

# MySQL database credentials
MYSQL_USER=root
MYSQL_PASSWORD=your_mysql_password
MYSQL_HOST=localhost
MYSQL_DATABASE=simplystock